{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "headers": {
        "Access-Control-Allow-Origin": "https://ai-chat-bot-hhui.vercel.app"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ]
}
